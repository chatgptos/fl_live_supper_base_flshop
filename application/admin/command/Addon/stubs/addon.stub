<?php

namespace addons\{%name%};

use app\common\library\Menu;
use think\Addons;

/**
 * 服务
 */
class {%addonClassName%} extends Addons
{

    /**
     * 服务安装方法
     * @return bool
     */
    public function install()
    {
        {%addonInstallMenu%}
        return true;
    }

    /**
     * 服务卸载方法
     * @return bool
     */
    public function uninstall()
    {
        {%addonUninstallMenu%}
        return true;
    }

    /**
     * 服务启用方法
     * @return bool
     */
    public function enable()
    {
        {%addonEnableMenu%}
        return true;
    }

    /**
     * 服务禁用方法
     * @return bool
     */
    public function disable()
    {
        {%addonDisableMenu%}
        return true;
    }

}
